> // error reporting
> 
> void
> sc_event_finder::report_error( const char* id, const char* add_msg ) const
> {
>     char msg[BUFSIZ];
>     if( add_msg != 0 ) {
> 	std::sprintf( msg, "%s: port '%s' (%s)",
